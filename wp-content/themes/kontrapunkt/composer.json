{
  "name": "porta/dva",
  "description": "Theme based on Timber with Styleguide",
  "authors": [
    {
      "name": "Å imon Macek",
      "email": "simon.macek@portadesign.cz"
    },
    {
      "name": "Petr Parimucha",
      "email": "petr.parimucha@portadesign.cz"
    },
    {
      "name": "Veronika Cepcekova",
      "email": "veronika.cepcekova@portadesign.cz"
    }
  ],
  "repositories": [
    {
      "type": "path",
      "url": "static"
    }
  ],
  "require": {
    "timber/timber": "^1.17",
    "hellonico/timber-dump-extension": "^1.0",
    "djboris88/timber-commented-include": "^1.1",
    "joachim-n/composer-manifest": "^1.1",
    "porta/static_base": "^1.0",
    "stoutlogic/acf-builder": "^1.11"
  },
  "autoload": {
    "classmap": [
      "classes"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal/core-composer-scaffold": true,
      "joachim-n/composer-manifest": true
    }
  }
}
